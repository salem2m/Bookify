@{
    ViewData["Title"] = "Reports";
}
<div class="d-flex flex-wrap flex-stack my-5">
    <!--begin::Heading-->
    <h3 class="fw-bold my-2">
        Reports
    </h3>
    <!--end::Heading-->
</div>
<div class="row g-6 g-xl-9 mb-6 mb-xl-9">
    <!--begin::Col-->
    <div class="col-md-6 col-lg-4 col-xl-3">
        <!--begin::Card-->
        <div class="card h-100 ">
            <!--begin::Card body-->
            <div class="card-body d-flex justify-content-center text-center flex-column p-8">
                <!--begin::Name-->
                <a asp-action="Books" class="text-gray-800 text-hover-primary d-flex flex-column">
                    <!--begin::Image-->
                    <div class="symbol symbol-60px mb-5">
                        <img src="~/assets/images/analytics.png" class="theme-light-show" alt="report">
                        <img src="~/assets/images/analytics.png" class="theme-dark-show" alt="report">

                    </div>
                    <!--end::Image-->
                    <!--begin::Title-->
                    <div class="fs-5 fw-bold mb-2">
                        Books
                    </div>
                    <!--end::Title-->
                </a>
                <!--end::Name-->
            </div>
            <!--end::Card body-->
        </div>
        <!--end::Card-->
    </div>
    <!--end::Col-->
    <!--begin::Col-->
    <div class="col-md-6 col-lg-4 col-xl-3">
        <!--begin::Card-->
        <div class="card h-100 ">
            <!--begin::Card body-->
            <div class="card-body d-flex justify-content-center text-center flex-column p-8">
                <!--begin::Name-->
                <a asp-action="Rentals" class="text-gray-800 text-hover-primary d-flex flex-column">
                    <!--begin::Image-->
                    <div class="symbol symbol-60px mb-5">
                        <img src="~/assets/images/analytics.png" class="theme-light-show" alt="report">
                        <img src="~/assets/images/analytics.png" class="theme-dark-show" alt="report">

                    </div>
                    <!--end::Image-->
                    <!--begin::Title-->
                    <div class="fs-5 fw-bold mb-2">
                        Rentals
                    </div>
                    <!--end::Title-->
                </a>
                <!--end::Name-->
            </div>
            <!--end::Card body-->
        </div>
        <!--end::Card-->
    </div>
    <!--end::Col-->
    <!--begin::Col-->
    <div class="col-md-6 col-lg-4 col-xl-3">
        <!--begin::Card-->
        <div class="card h-100 ">
            <!--begin::Card body-->
            <div class="card-body d-flex justify-content-center text-center flex-column p-8">
                <!--begin::Name-->
                <a asp-action="DelyedRentals" class="text-gray-800 text-hover-primary d-flex flex-column">
                    <!--begin::Image-->
                    <div class="symbol symbol-60px mb-5">
                        <img src="~/assets/images/analytics.png" class="theme-light-show" alt="report">
                        <img src="~/assets/images/analytics.png" class="theme-dark-show" alt="report">

                    </div>
                    <!--end::Image-->
                    <!--begin::Title-->
                    <div class="fs-5 fw-bold mb-2">
                        Delayed Rentals
                    </div>
                    <!--end::Title-->
                </a>
                <!--end::Name-->
            </div>
            <!--end::Card body-->
        </div>
        <!--end::Card-->
    </div>
    <!--end::Col-->
</div>